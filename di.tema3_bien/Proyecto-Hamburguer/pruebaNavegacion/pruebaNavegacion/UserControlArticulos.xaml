<UserControl x:Class="pruebaNavegacion.UserControlArticulos"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:pruebaNavegacion"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel Background="White">
        <Grid DockPanel.Dock="Top" >
            <StackPanel Orientation="Horizontal">
                <Button Content="Agregar Modelo Artíclo" FontSize="12" Width="200" Height="40" Background="Blue" Foreground="White" Click="CrearModeloArticulo_Click"/>
                <Button Margin="10,0,0,0"  Content="Agregar Artíclo" FontSize="12" Width="200" Height="40" Background="Blue" Foreground="White" Click="CrearArticulo_Click"/>
            </StackPanel>
        </Grid>
        <Grid>

        </Grid>
    </DockPanel>
</UserControl>
